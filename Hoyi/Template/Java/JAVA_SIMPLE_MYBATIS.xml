<!-- 用于注明生成各个文件是从哪个模板里面取值，并且放到哪个路径下面. -->
<Docments>
	<!-- mainOperate -->
	<!-- 左边是树，右边是操作的模板对自循环有用， 非自循环左边的树不能生成.  -->

	<Document id="entity" Template="JAVA_SIMPLE_MYBATIS/model/tb_user.java" TargetURL="[TargetURL]/model/[e.ClassName].java" />
	<Document id="modelxml" Template="JAVA_SIMPLE_MYBATIS/model/tb_user.xml" TargetURL="[TargetURL]/model/[e.ClassName].xml" />

	<Document id="dao" Template="JAVA_SIMPLE_MYBATIS/dao/Itb_userDao.java" TargetURL="[TargetURL]/dao/I[e.ClassName]Dao.java" />
	<Document id="html" Template="JAVA_SIMPLE_MYBATIS/WebRoot/mds/tb_use/tb_usemanag.html" TargetURL="[TargetURL]/WebRoot/mds/[e.ClassName]/[e.ClassName]manag.html" />

	<Document id="servlet" Template="JAVA_SIMPLE_MYBATIS/mds/tb_use/tb_userServlet.java" TargetURL="[TargetURL]/mds/[e.ClassName]/[e.ClassName]Servlet.java" />

	<Document id="webxml" Template="JAVA_SIMPLE_MYBATIS/WebRoot/WEB-INF/web.xml" TargetURL="[TargetURL]/web.xml" />

	<Document id="mybatisconfig" Template="JAVA_SIMPLE_MYBATIS/mybatis-config.xml" TargetURL="[TargetURL]/mybatis-config.xml" />

	<Document id="menujs" Template="JAVA_SIMPLE_MYBATIS/WebRoot/js/conf/sys-menu.js" TargetURL="[TargetURL]/sys-menu.js" />




	<Document id="SERVCONFIG" Template="JAVA_SIMPLE_MYBATIS/SERVCONFIG.java" TargetURL="[TargetURL]/SERVCONFIG.java" />








</Docments>
